<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Property.
    -->
    <changeSet id="20201122171028-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="sales_property_id"
                                 baseTableName="property"
                                 constraintName="fk_property_sales_property_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sales_property"/>

        <addForeignKeyConstraint baseColumnNames="category_id"
                                 baseTableName="property"
                                 constraintName="fk_property_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="category"/>

    </changeSet>
</databaseChangeLog>
