<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity AttributeValue.
    -->
    <changeSet id="20201122171528-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="attribute_id"
                                 baseTableName="attribute_value"
                                 constraintName="fk_attribute_value_attribute_id"
                                 referencedColumnNames="id"
                                 referencedTableName="attribute"/>

        <addForeignKeyConstraint baseColumnNames="property_id"
                                 baseTableName="attribute_value"
                                 constraintName="fk_attribute_value_property_id"
                                 referencedColumnNames="id"
                                 referencedTableName="property"/>

    </changeSet>
</databaseChangeLog>
